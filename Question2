#include <iostream>
using namespace std;
int main(){
    int t,s=0;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a=n/2;
        for (int i = 0; i<a; i++)
        {
            s+=n+1;
            cout<<s<<endl;
            n--;
        }
        
        cout<<s<<endl;
    }
    return 0;
}
